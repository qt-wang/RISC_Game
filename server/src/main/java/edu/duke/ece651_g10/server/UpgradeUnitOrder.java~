package edu.duke.ece651_g10.server;

public class UpgradeUnitOrder extends Order {
  private String dest;
  private Integer unitNum;
  private GameMap gmap;
  private Integer level;

  public UpgradeUnitOrder(int playerID, String dest, Integer unitNum, GameMap gMap, Integer level) {
    super(playerID);
    this.dst_Terr = dst_terr;
    this.unitNum = unitnum;
    this.level = level;
    this.gmap = gamemap;
  }
}









